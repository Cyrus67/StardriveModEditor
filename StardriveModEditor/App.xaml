<Application x:Class="StardriveModEditor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
             Startup="OnStart">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/BrushDefinitions.xaml"/>
                <ResourceDictionary Source="Resources/BasicControlStyles.xaml"/>
                <ResourceDictionary Source="Resources/WindowStyles.xaml"/>
                <ResourceDictionary Source="Resources/MenuStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
